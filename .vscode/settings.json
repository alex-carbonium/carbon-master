{
    "javascript.validate.enable": false,
    "eslint.enable": false,
    "jshint.enable": false,
    
    "search.exclude": {
        "**/node_modules": true,
        "**/target/": true     
    },
    "files.exclude": {
        "**/.git": true,
        "**/.idea": true,
        "**/.DS_Store": true,
        "**/node_modules": true
    },

    "editor.trimAutoWhitespace": true
}